{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "westus3"
    },
    "defaultCommands": {
      "value": [
        "cd /mount",
        "pip install -r ./setup/requirements.txt",
        "[ \"$hftoken\" != \"null\" ] && huggingface-cli login --token \"$hftoken\"",
        "huggingface-cli download mistralai/Mistral-7B-v0.1 --local-dir ./model-cache/mistralai/Mistral-7B --local-dir-use-symlinks False",
        "python3 ./finetuning/invoke_olive.py"
      ]
    },
    "maximumInstanceCount": {
      "value": 2
    },
    "huggingfaceTokenSecretName": {
      "value": "hftoken"
    },
    "storageAccountName": {
      "value": null
    },
    "fileShareName": {
      "value": null
    },
    "acaEnvironmentName": {
      "value": null
    },
    "acaEnvironmentStorageName": {
      "value": null
    },
    "acaJobName": {
      "value": null
    },
    "acaLogAnalyticsName": {
      "value": null
    }
  }
}