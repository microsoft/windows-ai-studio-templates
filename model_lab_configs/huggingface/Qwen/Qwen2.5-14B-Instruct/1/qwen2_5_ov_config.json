{
    "input_model": { "type": "HfModel", "model_path": "Qwen/Qwen2.5-14B-Instruct" },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "accelerators": [ { "execution_providers": [ "OpenVINOExecutionProvider" ] } ]
        }
    },
    "passes": {
        "optimum_convert": {
            "type": "OpenVINOOptimumConversion",
            "extra_args": { "device": "gpu" },
            "ov_quant_config": {
                "task": "text-generation-with-past",
                "weight_format": "int4",
                "group_size": 128,
                "ratio": 0.8
            }
        },
        "io_update": { "type": "OpenVINOIoUpdate", "static": false },
        "encapsulation": {
            "type": "OpenVINOEncapsulation",
            "target_device": "gpu",
            "keep_ov_dynamic_dims": true,
            "ov_version": "2025.1"
        }
    },
    "search_strategy": false,
    "host": "local_system",
    "target": "local_system",
    "cache_dir": "cache",
    "evaluate_input_model": false,
    "output_dir": "model/qwen2_5"
}
