{
    "runtime": {
        "name": "Optimize for",
        "description": "Currently this only determines the evaluation hardware",
        "type": "enum",
        "displayNames": [
            "Qualcomm NPU",
            "CPU"
        ],
        "path": "systems.qnn_system.accelerators.0.execution_providers.0",
        "values": [
            "QNNExecutionProvider",
            "CPUExecutionProvider"
        ]
    },
    "sections": [
        {
            "name": "Quantize",
            "parameters": [
                {
                    "name": "Activation Type",
                    "description": "Quantization data type of activation",
                    "type": "enum",
                    "path": "passes.OnnxQuantization.activation_type",
                    "values": [
                        "QInt8",
                        "QUInt8",
                        "QInt16",
                        "QUInt16"
                    ],
                    "template": {
                        "path": "passes.OnnxQuantization.activation_type",
                        "template": "ActivationType"
                    }
                },
                {
                    "name": "Weight Type",
                    "description": "Data type for quantizing weights",
                    "type": "enum",
                    "path": "passes.OnnxQuantization.weight_type",
                    "values": [
                        "QInt8",
                        "QUInt8",
                        "QInt16",
                        "QUInt16"
                    ],
                    "template": {
                        "path": "passes.OnnxQuantization.weight_type",
                        "template": "WeightType"
                    }
                },
                {
                    "name": "Quantization Dataset",
                    "type": "enum",
                    "path": "data_configs[0].load_dataset_config.data_name",
                    "values": [
                        "imagenet-1k"
                    ]
                },
                {
                    "name": "Quantization Dataset Split",
                    "type": "enum",
                    "path": "data_configs[0].load_dataset_config.split",
                    "values": [
                        "train",
                        "validation",
                        "test"
                    ]
                },
                {
                    "name": "Quantization Dataset Size",
                    "type": "int",
                    "path": "data_configs[0].pre_process_data_config.size",
                    "template": {
                        "path": "data_configs[0].pre_process_data_config.size",
                        "template": "QuantizationDatasetSize"
                    }
                }
            ],
            "toggle": {
                "name": "Quantize model",
                "type": "bool",
                "checks": [
                    {
                        "type": "exist",
                        "path": "passes.OnnxQuantization"
                    },
                    {
                        "type": "notExist",
                        "path": "passes.OnnxQuantization"
                    }
                ],
                "actions": [
                    [],
                    [
                        {
                            "type": "delete",
                            "path": "passes.QNNPreprocess"
                        },
                        {
                            "type": "delete",
                            "path": "passes.OnnxQuantization"
                        }
                    ]
                ]
            }
        },
        {
            "name": "Evaluate",
            "parameters": [
                {
                    "name": "Evaluation Dataset",
                    "type": "enum",
                    "path": "data_configs[1].load_dataset_config.data_name",
                    "values": [
                        "imagenet-1k"
                    ]
                },
                {
                    "name": "Evaluation Dataset Split",
                    "type": "enum",
                    "path": "data_configs[1].load_dataset_config.split",
                    "values": [
                        "train",
                        "validation",
                        "test"
                    ]
                },
                {
                    "name": "Evaluation Dataset Size",
                    "type": "int",
                    "path": "data_configs[1].pre_process_data_config.size",
                    "template": {
                        "path": "data_configs[1].pre_process_data_config.size",
                        "template": "EvaluationDatasetSize"
                    }
                }
            ],
            "toggle": {
                "name": "Evaluate model performance",
                "type": "bool",
                "checks": [
                    {
                        "type": "exist",
                        "path": "evaluator"
                    },
                    {
                        "type": "notExist",
                        "path": "evaluator"
                    }
                ],
                "actions": [
                    [],
                    [
                        {
                            "type": "delete",
                            "path": "evaluator"
                        }
                    ]
                ]
            }
        }
    ]
}