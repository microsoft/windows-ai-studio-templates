{
    "phases": [
        "Conversion",
        "Quantization",
        "Evaluation"
    ],
    "runtime": {
        "name": "Evaluate on",
        "type": "enum",
        "displayNames": [
            "CPU"
        ],
        "path": "systems.local_system.accelerators.0.execution_providers.0",
        "values": [
            "CPUExecutionProvider"
        ],
        "fixed": false
    },
    "sections": [
        {
            "name": "Convert",
            "parameters": [],
            "toggle": {
                "name": "Convert to ONNX format",
                "type": "bool",
                "path": "passes.conversion",
                "actions": [
                    [],
                    []
                ],
                "fixed": true
            }
        },
        {
            "name": "Quantize",
            "parameters": [
                {
                    "name": "Activation Type",
                    "type": "enum",
                    "displayType": "RadioGroup",
                    "path": "passes.quantization.activation_type",
                    "values": [
                        "QInt8",
                        "QUInt8",
                        "QInt16",
                        "QUInt16"
                    ]
                },
                {
                    "name": "Weight Type",
                    "type": "enum",
                    "displayType": "RadioGroup",
                    "path": "passes.quantization.weight_type",
                    "values": [
                        "QInt8",
                        "QUInt8",
                        "QInt16",
                        "QUInt16"
                    ]
                },
                {
                    "name": "Quantization Dataset Size",
                    "type": "int",
                    "path": "data_configs[0].pre_process_data_config.max_samples"
                }
            ],
            "toggle": {
                "name": "Quantize model",
                "type": "bool",
                "path": "passes.quantization",
                "actions": [
                    [],
                    [
                        {
                            "type": "upsert",
                            "path": "passes",
                            "value": {
                                "conversion": {
                                    "type": "OnnxConversion",
                                    "target_opset": 17,
                                    "save_as_external_data": true
                                }
                            }
                        }
                    ]
                ]
            }
        },
        {
            "name": "Evaluate",
            "parameters": [
                {
                    "name": "Evaluation Dataset Size",
                    "type": "int",
                    "path": "data_configs[1].pre_process_data_config.max_samples"
                }
            ],
            "toggle": {
                "name": "Evaluate model performance",
                "type": "bool",
                "path": "evaluator",
                "actions": [
                    [],
                    [
                        {
                            "type": "delete",
                            "path": "evaluator"
                        }
                    ]
                ]
            }
        }
    ]
}